<p id="player">Welcome <% @name %>, lets play</p>
<div id="game_box">
  <p><%= @move %></p>
  <p><%= @result %></p>
  <form action="/play" method="post">
    <label for="move_choice">Rock, Paper or Scissors</label>
      <input id="move_choice" name="move_choice" type="text"/>
    <button id="choose_move">Go</button>
  </form>
</div>
